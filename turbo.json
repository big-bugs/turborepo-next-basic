{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "pipeline": {
    "build": {
      "outputs": [".next/**", "!.next/cache/**"],
      "env": ["STRIPE_SECRET_KEY"]
    },
    "globalEnv": ["GITHUB_TOKEN"],
    "lint": {},
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
